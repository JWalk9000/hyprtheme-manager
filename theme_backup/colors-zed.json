{
    "$schema": "https://zed.dev/schema/themes/v0.1.0.json",
    "name": "pywal",
    "author": "pywal",
    "themes": [
        {
            "name": "pywal",
            "appearance": "dark",
            "style": {
                "background": "#0c141d90",
                "editor.background": "#0c141d90",
                "editor.foreground": "#c2c4c6",
                "text": "#c2c4c6",
                "text.muted": "#c2c4c670",
                "text.ignored": "#c2c4c640",
                "text.placeholder": "#c2c4c650",
                "ignored": "#c2c4c630",
                "element.hover": "#c2c4c630",
                "ghost_element.hover": "#857B2D30",
                "ghost_element.selected": "#857B2D30",
                "ghost_element.active": "#857B2D60",
                "border": "#0c141d",
                "editor.highlighted_line.background": "#857B2D10",
                "editor.active_line.background": "#857B2D10",
                "panel.background": "#0c141d90",
                "title_bar.background": "#0c141d90",
                "title_bar.inactive_background": "#0c141d90",
                "status_bar.background": "#0c141d90",
                "drop_target.background": "#0c141d90",
                "elevated_surface.background": "#0c141d",
                "toolbar.background": "#0c141d90",
                "tab_bar.background": "#0c141d90",
                "tab.inactive_background": "#0c141d90",
                "tab.active_background": "#857B2D30",
                "scrollbar.track.background": "transparent",
                "scrollbar.track.border": "#0c141d",
                "scrollbar.thumb.background": "#857B2D",
                "editor.gutter.background": "#0c141d90",
                "terminal.background": "#0c141d10",
                "terminal.foreground": "#c2c4c6",
                "terminal.dim_foreground": "#c2c4c6",
                "terminal.bright_foreground": "#c2c4c6",
                "terminal.ansi.black": "#0c141d",
                "terminal.ansi.red": "#857B2D",
                "terminal.ansi.green": "#8A7767",
                "terminal.ansi.yellow": "#958F38",
                "terminal.ansi.blue": "#9F8A77",
                "terminal.ansi.magenta": "#738484",
                "terminal.ansi.cyan": "#AF9985",
                "terminal.ansi.white": "#c2c4c6",
                "terminal.ansi.bright_black": "#5c6570",
                "terminal.ansi.bright_red": "#857B2D",
                "terminal.ansi.bright_green": "#8A7767",
                "terminal.ansi.bright_yellow": "#958F38",
                "terminal.ansi.bright_blue": "#9F8A77",
                "terminal.ansi.bright_magenta": "#738484",
                "terminal.ansi.bright_cyan": "#AF9985",
                "terminal.ansi.bright_white": "#c2c4c6",
                "modified": "#857B2D",
                "syntax": {
                    "attribute": {
                        "color": "#857B2D"
                    },
                    "boolean": {
                      "color": "#8A7767"
                    },
                    "comment": {
                      "color": "#5c6570"
                    },
                    "comment.doc": {
                      "color": "#5c6570"
                    },
                    "constant": {
                      "color": "#8A7767"
                    },
                    "function": {
                      "color": "#AF9985"
                    },
                    "keyword": {
                      "color": "#9F8A77"
                    },
                    "number": {
                      "color": "#738484"
                    },
                    "operator": {
                      "color": "#9F8A77"
                    },
                    "string": {
                      "color": "#857B2D"
                    },
                    "variable": {
                      "color": "#8A7767"
                    }
                },
                "players": [
                  {
                    "cursor": "#857B2D",
                    "background": "#0c141d",
                    "selection": "#857B2D30"
                  }
                ]
            }
        }
    ]
}
